{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "SoloHtml/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/",
            "dest": "/SoloHtml/Finanzas.html"
        },
        {
            "src": "/(.*)",
            "dest": "/SoloHtml/$1"
        }
    ]
}